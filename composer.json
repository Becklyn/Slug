{
    "name": "becklyn/slug",
    "type": "symfony-bundle",
    "description": "Everything about slugs for Symfony.",
    "homepage": "https://github.com/Becklyn/Slug",
    "license": "BSD-3-Clause",
    "authors": [
        {
            "name": "Becklyn Studios",
            "homepage": "http://becklyn.com/"
        }
    ],
    "require": {
        "php": ">=7.4",
        "ext-mbstring": "*",
        "symfony/config": "^4.4 || ^5.4",
        "symfony/dependency-injection": "^4.4 || ^5.4",
        "symfony/http-kernel": "^4.4 || ^5.4"
    },
    "require-dev": {
        "roave/security-advisories": "dev-master",
        "symfony/phpunit-bridge": "^5.0"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.x-dev",
            "dev-next": "1.x-dev"
        }
    },
    "autoload": {
        "psr-4": {
            "Becklyn\\Slug\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\Becklyn\\Slug\\": "tests/"
        }
    }
}
