{
    "name": "becklyn/slug",
    "type": "symfony-bundle",
    "description": "Everything about slugs for Symfony.",
    "homepage": "https://github.com/Becklyn/Slug",
    "license": "BSD-3-Clause",
    "authors": [
        {
            "name": "Becklyn Studios",
            "homepage": "http://becklyn.com/"
        }
    ],
    "require": {
        "php": "^7.2",
        "symfony/config": "^4.3 || ^5.0",
        "symfony/dependency-injection": "^4.3 || ^5.0",
        "symfony/http-kernel": "^4.3 || ^5.0"
    },
    "require-dev": {
        "becklyn/php-cs": "^1.1"
    },
    "config": {
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "Becklyn\\Slug\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\Becklyn\\Slug\\": "tests/"
        }
    }
}
